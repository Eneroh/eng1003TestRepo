// From lecture a, slide 9
// switch example

int main()  {
    int x = 1, y = 2;

    switch(x == y)  {
        case 0:
            printf("x and y differ\n");
            break;
        case 1:
            printf("x and y are equal\n");
            break;
        default:
            printf("Something went very wrong\n)");
    }
}

